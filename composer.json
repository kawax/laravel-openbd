{
    "name": "revolution/laravel-openbd",
    "description": "OpenBD API",
    "keywords": [
        "openbd",
        "laravel"
    ],
    "type": "library",
    "require": {
        "php": "^7.2||^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3||^7.0",
        "illuminate/support": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0||^9.0",
        "mockery/mockery": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Revolution\\OpenBD\\": "src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "kawax",
            "email": "kawaxbiz@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Revolution\\OpenBD\\Providers\\OpenBDServiceProvider"
            ],
            "aliases": {
                "OpenBD": "Revolution\\OpenBD\\Facades\\OpenBD"
            }
        }
    }
}
